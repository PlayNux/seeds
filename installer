 * nic-shared-modules-${Kernel-Version}-di
 * nic-usb-modules-${Kernel-Version}-di
 * ntfs-modules-${Kernel-Version}-di
 * parport-modules-${Kernel-Version}-di
 * pcmcia-modules-${Kernel-Version}-di
 * pcmcia-storage-modules-${Kernel-Version}-di
 * plip-modules-${Kernel-Version}-di
 * ppp-modules-${Kernel-Version}-di
 * sata-modules-${Kernel-Version}-di
